#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/arlonwilber/Library/CloudStorage/GoogleDrive-awilber@wiredtriangle.com/Shared drives/AW/Personal/Projects/jarvis/node_modules/.pnpm/playwright-core@1.36.2/node_modules/playwright-core/node_modules:/Users/arlonwilber/Library/CloudStorage/GoogleDrive-awilber@wiredtriangle.com/Shared drives/AW/Personal/Projects/jarvis/node_modules/.pnpm/playwright-core@1.36.2/node_modules:/Users/arlonwilber/Library/CloudStorage/GoogleDrive-awilber@wiredtriangle.com/Shared drives/AW/Personal/Projects/jarvis/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/arlonwilber/Library/CloudStorage/GoogleDrive-awilber@wiredtriangle.com/Shared drives/AW/Personal/Projects/jarvis/node_modules/.pnpm/playwright-core@1.36.2/node_modules/playwright-core/node_modules:/Users/arlonwilber/Library/CloudStorage/GoogleDrive-awilber@wiredtriangle.com/Shared drives/AW/Personal/Projects/jarvis/node_modules/.pnpm/playwright-core@1.36.2/node_modules:/Users/arlonwilber/Library/CloudStorage/GoogleDrive-awilber@wiredtriangle.com/Shared drives/AW/Personal/Projects/jarvis/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../playwright-core@1.36.2/node_modules/playwright-core/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../playwright-core@1.36.2/node_modules/playwright-core/cli.js" "$@"
fi
