else
  export NODE_PATH="/Users/arlonwilber/Library/CloudStorage/GoogleDrive-awilber@wiredtriangle.com/Shared drives/AW/Personal/Projects/jarvis/node_modules/.pnpm/esbuild@0.18.20/node_modules/esbuild/bin/node_modules:/Users/arlonwilber/Library/CloudStorage/GoogleDrive-awilber@wiredtriangle.com/Shared drives/AW/Personal/Projects/jarvis/node_modules/.pnpm/esbuild@0.18.20/node_modules/esbuild/node_modules:/Users/arlonwilber/Library/CloudStorage/GoogleDrive-awilber@wiredtriangle.com/Shared drives/AW/Personal/Projects/jarvis/node_modules/.pnpm/esbuild@0.18.20/node_modules:/Users/arlonwilber/Library/CloudStorage/GoogleDrive-awilber@wiredtriangle.com/Shared drives/AW/Personal/Projects/jarvis/node_modules/.pnpm/node_modules:$NODE_PATH"
